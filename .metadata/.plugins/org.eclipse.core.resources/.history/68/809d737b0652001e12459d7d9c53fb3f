package pkg1basic_practices;
//1.types of variables (static ,global,local)

class VariableTypes {
	static int static_v = 4;
	int global_v = 5;

	void method1() {
		int local_v = 6;
		System.out.println(local_v);
	}
}

public class MainVariables {
	public static void main(String[] args) {
		VariableTypes vt = new VariableTypes();
		
		System.out.println(VariableTypes.static_v);
		
		System.out.println(vt.global_v);
		
		vt.method1();
	
	
	}
}
